            "id": "http-1",
            "name": "Directory Brute Force",
            "difficulty": "Beginner",
            "description": "Discover hidden directories and files on web servers",
            "steps": [
                "Select appropriate wordlist",
                "Configure scanning tool",
                "Launch directory scan",
                "Analyze discovered paths"
            ],
            "commands": [
                {
                    "tool": "Gobuster",
                    "command": "gobuster dir -u http://<target_ip> -w /usr/share/wordlists/dirb/common.txt",
                    "description": "Fast directory brute forcing"
                },
                {
                    "tool": "Feroxbuster",
                    "command": "feroxbuster -u http://<target_ip> -w wordlist.txt",
                    "description": "Recursive directory discovery"
                },
                {
                    "tool": "ffuf",
                    "command": "ffuf -u http://<target_ip>/FUZZ -w wordlist.txt",
                    "description": "Fast web fuzzer"
                },
                {
                    "tool": "Dirsearch",
                    "command": "dirsearch -u http://<target_ip> -e php,html,txt",
                    "description": "Python directory scanner"
                }
            ],
            "tips": [
                "Use multiple wordlists for thorough coverage",
                "Check for backup files (.bak, .old, ~)",
                "Look for admin panels, .git folders"
            ]
        },
        {
            "id": "http-2",
            "name": "SQL Injection",
            "difficulty": "Intermediate",
